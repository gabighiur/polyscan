this.mediaItems.length;\r\n            },\r\n        },\r\n    };\r\n</script>\r\n\r\n<style scoped>\r\n    /* Your custom styles for the SliderComponent */\r\n\r\n    .custom-image-container,\r\n    .custom-video-container {\r\n        width: 100%;\r\n        height: 100%;\r\n    }\r\n\r\n        .custom-image-container img,\r\n        .custom-video-container video {\r\n            width: 100%;\r\n            height: 100%;\r\n            object-fit: cover;\r\n        }\r\n</style>\r\n"],"mappings":"AA4CI,eAAe;EACXA,IAAI,EAAE,iBAAiB;EACvBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,UAAU,EAAE,CACR;QACIC,EAAE,EAAE,CAAC;QACLC,IAAI,EAAE,OAAO;QACbC,GAAG,EAAE;MACT,CAAC,EACD;QACIF,EAAE,EAAE,CAAC;QACLC,IAAI,EAAE,OAAO;QACbC,GAAG,EAAE;MACT,CAAC,EACD;QACIF,EAAE,EAAE,CAAC;QACLC,IAAI,EAAE,OAAO;QACbC,GAAG,EAAE;MACT,CAAC,CACJ;MACDC,WAAW,EAAE;IACjB,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACLC,SAASA,CAAA,EAAG;MACR,IAAI,CAACF,WAAU,GACX,CAAC,IAAI,CAACA,WAAU,GAAI,IAAI,IAAI,CAACJ,UAAU,CAACO,MAAM,IAAI,IAAI,CAACP,UAAU,CAACO,MAAM;IAChF,CAAC;IACDC,SAASA,CAAA,EAAG;MACR,IAAI,CAACJ,WAAU,GAAI,CAAC,IAAI,CAACA,WAAU,GAAI,CAAC,IAAI,IAAI,CAACJ,UAAU,CAACO,MAAM;IACtE;EACJ;AACJ,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}