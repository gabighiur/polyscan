{"ast":null,"code":"import WhatCanIDo from './WhatCanIDo.vue';\nexport default {\n  name: 'PolygonScanner',\n  components: {\n    WhatCanIDo\n  },\n  data() {\n    return {\n      showWhatCanIDo: false\n    };\n  },\n  mounted() {\n    this.initializeIntersectionObserver();\n    window.addEventListener('scroll', this.handleScroll);\n  },\n  beforeUnmount() {\n    this.unobserveElements();\n    window.removeEventListener('scroll', this.handleScroll);\n  },\n  methods: {\n    toggleMenu() {\n      // Implement the logic to toggle the mobile menu here\n    },\n    initializeIntersectionObserver() {\n      this.intersectionObserver = new IntersectionObserver(entries => {\n        entries.forEach(entry => {\n          if (entry.isIntersecting) {\n            this.showWhatCanIDo = true;\n            this.intersectionObserver.unobserve(entry.target);\n          }\n        });\n      }, {\n        threshold: 0.3\n      } // Adjust the threshold as needed\n      );\n\n      this.observeElements();\n    },\n    observeElements() {\n      const elements = this.$el.querySelectorAll('.fade-in-container');\n      Array.from(elements).forEach(element => {\n        this.intersectionObserver.observe(element);\n        element.style.opacity = '0';\n      });\n    },\n    unobserveElements() {\n      const elements = this.$el.querySelectorAll('.fade-in-container');\n      Array.from(elements).forEach(element => {\n        this.intersectionObserver.unobserve(element);\n      });\n    },\n    handleScroll() {\n      const videoHeight = this.$el.querySelector('.header-video').offsetHeight;\n      const triggerOffset = this.$el.querySelector('.header').offsetTop + videoHeight;\n      if (window.pageYOffset >= triggerOffset) {\n        this.showWhatCanIDo = true;\n        window.removeEventListener('scroll', this.handleScroll);\n      } else {\n        this.showWhatCanIDo = false;\n      }\n    }\n  }\n};","map":{"version":3,"names":["WhatCanIDo","name","components","data","showWhatCanIDo","mounted","initializeIntersectionObserver","window","addEventListener","handleScroll","beforeUnmount","unobserveElements","removeEventListener","methods","toggleMenu","intersectionObserver","IntersectionObserver","entries","forEach","entry","isIntersecting","unobserve","target","threshold","observeElements","elements","$el","querySelectorAll","Array","from","element","observe","style","opacity","videoHeight","querySelector","offsetHeight","triggerOffset","offsetTop","pageYOffset"],"sources":["C:\\D\\Vue\\polyscan\\src\\components\\PolygonScanner.vue"],"sourcesContent":["<template>\r\n    <div class=\"app\">\r\n        <nav class=\"navbar\">\r\n            <div class=\"navbar-content\">\r\n                <img class=\"logo\" src=\"../assets/logo2.png\" alt=\"Logo\" />\r\n                <button class=\"invite-button\">Invite</button>\r\n            </div>\r\n            <button class=\"hamburger\" @click=\"toggleMenu\">\r\n                <span class=\"hamburger-line\"></span>\r\n                <span class=\"hamburger-line\"></span>\r\n                <span class=\"hamburger-line\"></span>\r\n            </button>\r\n        </nav>\r\n        <div class=\"header\">\r\n            <video class=\"header-video\" src=\"../assets/hi1.mp4\" autoplay loop muted></video>\r\n        </div>\r\n        <div class=\"content-container\">\r\n            <transition name=\"fade-in\">\r\n                <div class=\"fade-in-container\" v-if=\"showWhatCanIDo\">\r\n                    <WhatCanIDo />\r\n                </div>\r\n            </transition>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import WhatCanIDo from './WhatCanIDo.vue';\r\n\r\n    export default {\r\n        name: 'PolygonScanner',\r\n        components: {\r\n            WhatCanIDo,\r\n        },\r\n        data() {\r\n            return {\r\n                showWhatCanIDo: false,\r\n            };\r\n        },\r\n        mounted() {\r\n            this.initializeIntersectionObserver();\r\n            window.addEventListener('scroll', this.handleScroll);\r\n        },\r\n        beforeUnmount() {\r\n            this.unobserveElements();\r\n            window.removeEventListener('scroll', this.handleScroll);\r\n        },\r\n        methods: {\r\n            toggleMenu() {\r\n                // Implement the logic to toggle the mobile menu here\r\n            },\r\n            initializeIntersectionObserver() {\r\n                this.intersectionObserver = new IntersectionObserver(\r\n                    (entries) => {\r\n                        entries.forEach((entry) => {\r\n                            if (entry.isIntersecting) {\r\n                                this.showWhatCanIDo = true;\r\n                                this.intersectionObserver.unobserve(entry.target);\r\n                            }\r\n                        });\r\n                    },\r\n                    { threshold: 0.3 } // Adjust the threshold as needed\r\n                );\r\n\r\n                this.observeElements();\r\n            },\r\n            observeElements() {\r\n                const elements = this.$el.querySelectorAll('.fade-in-container');\r\n                Array.from(elements).forEach((element) => {\r\n                    this.intersectionObserver.observe(element);\r\n                    element.style.opacity = '0';\r\n                });\r\n            },\r\n            unobserveElements() {\r\n                const elements = this.$el.querySelectorAll('.fade-in-container');\r\n                Array.from(elements).forEach((element) => {\r\n                    this.intersectionObserver.unobserve(element);\r\n                });\r\n            },\r\n            handleScroll() {\r\n                const videoHeight = this.$el.querySelector('.header-video').offsetHeight;\r\n                const triggerOffset = this.$el.querySelector('.header').offsetTop + videoHeight;\r\n\r\n                if (window.pageYOffset >= triggerOffset) {\r\n                    this.showWhatCanIDo = true;\r\n                    window.removeEventListener('scroll', this.handleScroll);\r\n                } else {\r\n                    this.showWhatCanIDo = false;\r\n                }\r\n            },\r\n        },\r\n    };\r\n</script>\r\n\r\n<style scoped>\r\n    .app {\r\n        background: #27082B;\r\n        color: #67c4a5;\r\n        min-height: 100vh;\r\n        display: flex;\r\n        flex-direction: column;\r\n        align-items: center;\r\n    }\r\n\r\n    .navbar {\r\n        position: fixed;\r\n        top: 0;\r\n        left: 0;\r\n        width: 100%;\r\n        background-color: transparent;\r\n        color: #fff;\r\n        padding: 0.2rem;\r\n        display: flex;\r\n        justify-content: flex-end;\r\n        align-items: center;\r\n        z-index: 9999;\r\n    }\r\n\r\n    .navbar-content {\r\n        display: flex;\r\n        align-items: center;\r\n        justify-content: flex-end;\r\n        margin-right: 15px;\r\n    }\r\n\r\n    .logo {\r\n        width: 120px;\r\n        height: 120px;\r\n    }\r\n\r\n    .invite-button {\r\n        background-color: #67c4a5;\r\n        color: #fff;\r\n        border: none;\r\n        font-size: 1rem;\r\n        padding: 0.5rem 1rem;\r\n        margin-left: 1rem;\r\n        cursor: pointer;\r\n        border-radius: 10px;\r\n    }\r\n\r\n    .hamburger {\r\n        display: none;\r\n        background-color: transparent;\r\n        border: none;\r\n        cursor: pointer;\r\n    }\r\n\r\n    .hamburger-line {\r\n        display: block;\r\n        width: 30px;\r\n        height: 2px;\r\n        background-color: #fff;\r\n        margin: 5px;\r\n    }\r\n\r\n    .header {\r\n        position: relative;\r\n        width: 100%;\r\n        height: 100vh;\r\n        display: flex;\r\n        justify-content: center;\r\n        align-items: center;\r\n    }\r\n\r\n    .header-video {\r\n        max-width: 60%;\r\n        height: auto;\r\n        object-fit: cover;\r\n    }\r\n\r\n    .content-container {\r\n        margin-top: calc(50vh + 120px); /* Adjust the margin-top value based on your navbar height */\r\n        display: flex;\r\n        justify-content: center;\r\n        align-items: flex-start;\r\n    }\r\n\r\n    .fade-in-container {\r\n        opacity: 0;\r\n        transition: opacity 0.5s ease-in;\r\n    }\r\n\r\n    .fade-in-enter-active,\r\n    .fade-in-leave-active {\r\n        opacity: 1;\r\n    }\r\n\r\n    .fade-in-enter,\r\n    .fade-in-leave-to {\r\n        opacity: 0;\r\n    }\r\n</style>\r\n"],"mappings":"AA2BI,OAAOA,UAAS,MAAO,kBAAkB;AAEzC,eAAe;EACXC,IAAI,EAAE,gBAAgB;EACtBC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,cAAc,EAAE;IACpB,CAAC;EACL,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACC,8BAA8B,CAAC,CAAC;IACrCC,MAAM,CAACC,gBAAgB,CAAC,QAAQ,EAAE,IAAI,CAACC,YAAY,CAAC;EACxD,CAAC;EACDC,aAAaA,CAAA,EAAG;IACZ,IAAI,CAACC,iBAAiB,CAAC,CAAC;IACxBJ,MAAM,CAACK,mBAAmB,CAAC,QAAQ,EAAE,IAAI,CAACH,YAAY,CAAC;EAC3D,CAAC;EACDI,OAAO,EAAE;IACLC,UAAUA,CAAA,EAAG;MACT;IAAA,CACH;IACDR,8BAA8BA,CAAA,EAAG;MAC7B,IAAI,CAACS,oBAAmB,GAAI,IAAIC,oBAAoB,CAC/CC,OAAO,IAAK;QACTA,OAAO,CAACC,OAAO,CAAEC,KAAK,IAAK;UACvB,IAAIA,KAAK,CAACC,cAAc,EAAE;YACtB,IAAI,CAAChB,cAAa,GAAI,IAAI;YAC1B,IAAI,CAACW,oBAAoB,CAACM,SAAS,CAACF,KAAK,CAACG,MAAM,CAAC;UACrD;QACJ,CAAC,CAAC;MACN,CAAC,EACD;QAAEC,SAAS,EAAE;MAAI,EAAE;MACvB,CAAC;;MAED,IAAI,CAACC,eAAe,CAAC,CAAC;IAC1B,CAAC;IACDA,eAAeA,CAAA,EAAG;MACd,MAAMC,QAAO,GAAI,IAAI,CAACC,GAAG,CAACC,gBAAgB,CAAC,oBAAoB,CAAC;MAChEC,KAAK,CAACC,IAAI,CAACJ,QAAQ,CAAC,CAACP,OAAO,CAAEY,OAAO,IAAK;QACtC,IAAI,CAACf,oBAAoB,CAACgB,OAAO,CAACD,OAAO,CAAC;QAC1CA,OAAO,CAACE,KAAK,CAACC,OAAM,GAAI,GAAG;MAC/B,CAAC,CAAC;IACN,CAAC;IACDtB,iBAAiBA,CAAA,EAAG;MAChB,MAAMc,QAAO,GAAI,IAAI,CAACC,GAAG,CAACC,gBAAgB,CAAC,oBAAoB,CAAC;MAChEC,KAAK,CAACC,IAAI,CAACJ,QAAQ,CAAC,CAACP,OAAO,CAAEY,OAAO,IAAK;QACtC,IAAI,CAACf,oBAAoB,CAACM,SAAS,CAACS,OAAO,CAAC;MAChD,CAAC,CAAC;IACN,CAAC;IACDrB,YAAYA,CAAA,EAAG;MACX,MAAMyB,WAAU,GAAI,IAAI,CAACR,GAAG,CAACS,aAAa,CAAC