{"ast":null,"code":"export default {\n  name: 'MediaComponent',\n  data() {\n    return {\n      mediaItems: [{\n        id: 1,\n        type: 'image',\n        src: 'slider3.png'\n      }, {\n        id: 2,\n        type: 'video',\n        src: 'slider2.mp4'\n      }, {\n        id: 3,\n        type: 'video',\n        src: 'slider1.mp4'\n      }]\n    };\n  }\n};","map":{"version":3,"names":["name","data","mediaItems","id","type","src"],"sources":["C:\\D\\Vue\\polyscan\\src\\components\\SliderComponent.vue"],"sourcesContent":["<template>\r\n    <div class=\"media-container\">\r\n        <div v-for=\"item in mediaItems\" :key=\"item.id\" class=\"media-item fade-in\">\r\n            <div v-if=\"item.type === 'image'\" class=\"image-container\">\r\n                <img :src=\"require(`@/assets/jaluzele/${item.src}`)\" alt=\"Slide Image\" />\r\n            </div>\r\n            <div v-else-if=\"item.type === 'video'\" class=\"video-container\">\r\n                <video :src=\"require(`@/assets/jaluzele/${item.src}`)\" loop autoplay muted></video>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    export default {\r\n        name: 'MediaComponent',\r\n        data() {\r\n            return {\r\n                mediaItems: [\r\n                    {\r\n                        id: 1,\r\n                        type: 'image',\r\n                        src: 'slider3.png',\r\n                    },\r\n                    {\r\n                        id: 2,\r\n                        type: 'video',\r\n                        src: 'slider2.mp4',\r\n                    },\r\n                    {\r\n                        id: 3,\r\n                        type: 'video',\r\n                        src: 'slider1.mp4',\r\n                    },\r\n                ],\r\n            };\r\n        },\r\n    };\r\n</script>\r\n\r\n<style>\r\n    .media-container {\r\n        display: flex;\r\n        justify-content: center;\r\n        align-items: center;\r\n        height: 100vh;\r\n    }\r\n\r\n    .fade-in {\r\n        animation: fadeIn 1s ease-in-out forwards;\r\n    }\r\n\r\n    @keyframes fadeIn {\r\n        from {\r\n            opacity: 0;\r\n        }\r\n\r\n        to {\r\n            opacity: 1;\r\n        }\r\n    }\r\n\r\n    .image-container,\r\n    .video-container {\r\n        width: 100%;\r\n        height: 100%;\r\n    }\r\n\r\n        .image-container img,\r\n        .video-container video {\r\n            width: 100%;\r\n            height: 100%;\r\n            object-fit: cover;\r\n        }\r\n</style>\r\n"],"mappings":"AAcI,eAAe;EACXA,IAAI,EAAE,gBAAgB;EACtBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,UAAU,EAAE,CACR;QACIC,EAAE,EAAE,CAAC;QACLC,IAAI,EAAE,OAAO;QACbC,GAAG,EAAE;MACT,CAAC,EACD;QACIF,EAAE,EAAE,CAAC;QACLC,IAAI,EAAE,OAAO;QACbC,GAAG,EAAE;MACT,CAAC,EACD;QACIF,EAAE,EAAE,CAAC;QACLC,IAAI,EAAE,OAAO;QACbC,GAAG,EAAE;MACT,CAAC;IAET,CAAC;EACL;AACJ,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}