lAnimations, {\r\n                root: null,\r\n                rootMargin: '0px',\r\n                threshold: threshold,\r\n            });\r\n\r\n            const section1 = this.$refs.section1;\r\n            const section2 = this.$refs.section2;\r\n\r\n            observer.observe(section1);\r\n            observer.observe(section2);\r\n\r\n            const swiper = new Swiper('.carousel-slider', this.swiperOptions);\r\n            this.onSwiperInit(swiper);\r\n        },\r\n    };\r\n</script>\r\n\r\n<style scoped>\r\n    @import '../assets/styles.css';\r\n</style>\r\n"],"mappings":"AA8EI,OAAO,0BAA0B;AACjC,OAAOA,MAAK,MAAO,eAAe;AAElC,OAAOC,GAAE,MAAO,kBAAkB;AAClC,OAAOC,OAAM,MAAO,oBAAoB;AACxC,OAAOC,KAAI,MAAO,sBAAsB;AACxC,OAAOC,IAAG,MAAO,mBAAmB;AAEpC,eAAe;EACXC,IAAI,EAAE,aAAa;EAEnBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHJ,OAAO,EAAEA,OAAO;MAChBC,KAAK,EAAEA,KAAK;MACZC,IAAI,EAAEA,IAAI;MACVG,WAAW,EAAEN,GAAG;MAChBO,aAAa,EAAE,mBAAmB;MAClCC,aAAa,EAAE,qEAAqE;MACpFC,aAAa,EAAE,+CAA+C;MAC9DC,aAAa,EAAE,2CAA2C;MAC1DC,aAAa,EAAE,yBAAyB;MACxCC,aAAa,EAAE,+DAA+D;MAC9EC,aAAa,EAAE,+EAA+E;MAC9FC,WAAW,EAAE,CACT;QACIC,IAAI,EAAE,OAAO;QACbC,GAAG,EAAEC,OAAO,CAAC,+BAA+B;MAChD,CAAC,EACD;QACIF,IAAI,EAAE,OAAO;QACbC,GAAG,EAAEC,OAAO,CAAC,+BAA+B,CAAC;QAC7CC,IAAI,EAAE;MACV,CAAC,EACD;QACIH,IAAI,EAAE,OAAO;QACbC,GAAG,EAAEC,OAAO,CAAC,+BAA+B;MAChD,EACH;MACDE,WAAW,EAAE,CAAC;MACdC,aAAa,EAAE;QACXF,IAAI,EAAE,IAAI;QACVG,QAAQ,EAAE;UACNC,KAAK,EAAE,IAAI;UACXC,oBAAoB,EAAE;QAC1B,CAAC;QACDC,UAAU,EAAE;UACRC,EAAE,EAAE;QACR,CAAC;QACDC,UAAU,EAAE;UACRC,MAAM,EAAE,qBAAqB;UAC7BC,MAAM,EAAE;QACZ;MACJ;IACJ,CAAC;EACL,CAAC;EAEDC,OAAO,EAAE;IACLC,UAAUA,CAAA,EAAG;MACT,IAAI,CAACC,cAAa,GAAI,CAAC,IAAI,CAACA,cAAc;IAC9C,CAAC;IACDC,sBAAsBA,CAACC,OAAO,EAAE;MAC5BA,OAAO,CAACC,OAAO,CAAEC,KAAK,IAAK;QACvB,IAAIA,KAAK,CAACC,cAAc,EAAE;UACtBD,KAAK,CAACE,MAAM,CAACC,SAAS,CAACC,GAAG,CAAC,SAAS,CAAC;QACzC,OAAO;UACHJ,KAAK,CAACE,MAAM,CAACC,SAAS,CAACE,MAAM,CAAC,SAAS,CAAC;QAC5C;MACJ,CAAC,CAAC;IACN,CAAC;IACDC,YAAYA,CAACC,MAAM,EAAE;MACjBA,MAAM,CAACC,EAAE,CAAC