{"ast":null,"code":"import WhatCanIDo from './WhatCanIDo.vue';\nimport AOS from 'aos';\nimport 'aos/dist/aos.css';\nexport default {\n  name: 'PolygonScanner',\n  components: {\n    WhatCanIDo\n  },\n  data() {\n    return {\n      showWhatCanIDo: false\n    };\n  },\n  mounted() {\n    this.initializeAOS();\n    this.initializeIntersectionObserver();\n  },\n  beforeUnmount() {\n    this.unobserveElements();\n  },\n  methods: {\n    toggleMenu() {\n      // Implement the logic to toggle the mobile menu here\n    },\n    initializeAOS() {\n      AOS.init();\n    },\n    initializeIntersectionObserver() {\n      this.intersectionObserver = new IntersectionObserver(entries => {\n        entries.forEach(entry => {\n          if (entry.isIntersecting) {\n            this.showWhatCanIDo = true;\n            this.intersectionObserver.unobserve(entry.target);\n          }\n        });\n      }, {\n        threshold: 0.5\n      } // Adjust the threshold as needed\n      );\n\n      this.observeElements();\n    },\n    observeElements() {\n      const element = this.$refs.contentContainer;\n      this.intersectionObserver.observe(element);\n      element.style.opacity = '0';\n    },\n    unobserveElements() {\n      const element = this.$refs.contentContainer;\n      this.intersectionObserver.unobserve(element);\n    }\n  }\n};","map":{"version":3,"names":["WhatCanIDo","AOS","name","components","data","showWhatCanIDo","mounted","initializeAOS","initializeIntersectionObserver","beforeUnmount","unobserveElements","methods","toggleMenu","init","intersectionObserver","IntersectionObserver","entries","forEach","entry","isIntersecting","unobserve","target","threshold","observeElements","element","$refs","contentContainer","observe","style","opacity"],"sources":["C:\\D\\Vue\\polyscan\\src\\components\\PolygonScanner.vue"],"sourcesContent":["<template>\r\n    <div class=\"polygon-scanner\">\r\n        <nav class=\"navbar\">\r\n            <div class=\"navbar-content\">\r\n           