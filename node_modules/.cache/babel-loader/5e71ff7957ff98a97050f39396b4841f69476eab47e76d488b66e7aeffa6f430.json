{"ast":null,"code":"import SwiperCore, { Pagination } from 'swiper';\nimport './swiper/swiper-bundle.css';\nSwiperCore.use([Pagination]);\nexport default {\n  name: 'SliderComponent',\n  data() {\n    return {\n      swiperOptions: {\n        pagination: {\n          el: '.swiper-pagination'\n        },\n        loop: true\n      },\n      sliderItems: [{\n        id: 1,\n        type: 'image',\n        src: 'slider1.png'\n      }, {\n        id: 2,\n        type: 'video',\n        src: 'slider2.mp4'\n      }, {\n        id: 3,\n        type: 'video',\n        src: 'slider3.mp4'\n      }]\n    };\n  }\n};","map":{"version":3,"names":["SwiperCore","Pagination","use","name","data","swiperOptions","pagination","el","loop","sliderItems","id","type","src"],"sources":["C:\\D\\Vue\\polyscan\\src\\components\\SliderComponent.vue"],"sourcesContent":["<template>\r\n    <div class=\"slider\">\r\n        <swiper :options=\"swiperOptions\" ref=\"swiper\">\r\n            <swiper-slide v-for=\"item in sliderItems\" :key=\"item.id\">\r\n                <div v-if=\"item.type === 'image'\">\r\n                    <img :src=\"require(`@/assets/jaluzele/${item.src}`)\" alt=\"Slide Image\" />\r\n                </div>\r\n                <div v-else-if=\"item.type === 'video'\">\r\n                    <video :src=\"require(`@/assets/jaluzele/${item.src}`)\" loop autoplay muted></video>\r\n                </div>\r\n            </swiper-slide>\r\n            <template v-slot:pagination>\r\n                <div class=\"swiper-pagination\"></div>\r\n            </template>\r\n        </swiper>\r\n    </div>\r\n</template>\r\n\r\n\r\n<script>\r\n    import SwiperCore, { Pagination } from 'swiper';\r\n    import './swiper/swiper-bundle.css';\r\n\r\n    SwiperCore.use([Pagination]);\r\n\r\n    export default {\r\n        name: 'SliderComponent',\r\n        data() {\r\n            return {\r\n                swiperOptions: {\r\n                    pagination: {\r\n                        el: '.swiper-pagination',\r\n                    },\r\n                    loop: true,\r\n                },\r\n                sliderItems: [\r\n                    {\r\n                        id: 1,\r\n                        type: 'image',\r\n                        src: 'slider1.png',\r\n                    },\r\n                    {\r\n                        id: 2,\r\n                        type: 'video',\r\n                        src: 'slider2.mp4',\r\n                    },\r\n                    {\r\n                        id: 3,\r\n                        type: 'video',\r\n                        src: 'slider3.mp4',\r\n                    },\r\n                ],\r\n            };\r\n        },\r\n    };\r\n</script>\r\n\r\n<style scoped>\r\n    .slider {\r\n        width: 100%;\r\n        height: 400px;\r\n    }\r\n\r\n        .slider video {\r\n            width: 100%;\r\n            height: 100%;\r\n            object-fit: cover;\r\n        }\r\n</style>\r\n"],"mappings":"AAoBI,OAAOA,UAAU,IAAIC,UAAS,QAAS,QAAQ;AAC/C,OAAO,4BAA4B;AAEnCD,UAAU,CAACE,GAAG,CAAC,CAACD,UAAU,CAAC,CAAC;AAE5B,eAAe;EACXE,IAAI,EAAE,iBAAiB;EACvBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,aAAa,EAAE;QACXC,UAAU,EAAE;UACRC,EAAE,EAAE;QACR,CAAC;QACDC,IAAI,EAAE;MACV,CAAC;MACDC,WAAW,EAAE,CACT;QACIC,EAAE,EAAE,CAAC;QACLC,IAAI,EAAE,OAAO;QACbC,GAAG,EAAE;MACT,CAAC,EACD;QACIF,EAAE,EAAE,CAAC;QACLC,IAAI,EAAE,OAAO;QACbC,GAAG,EAAE;MACT,CAAC,EACD;QACIF,EAAE,EAAE,CAAC;QACLC,IAAI,EAAE,OAAO;QACbC,GAAG,EAAE;MACT,CAAC;IAET,CAAC;EACL;AACJ,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}