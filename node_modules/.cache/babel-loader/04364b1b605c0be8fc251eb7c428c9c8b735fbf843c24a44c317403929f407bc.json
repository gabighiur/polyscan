howMobileMenu;\r\n            },\r\n\r\n            handleScrollAnimations(entries) {\r\n                entries.forEach((entry) => {\r\n                    if (entry.isIntersecting) {\r\n                        entry.target.classList.add('visible');\r\n                    } else {\r\n                        entry.target.classList.remove('visible');\r\n                    }\r\n                });\r\n            },\r\n        },\r\n        mounted() {\r\n            const isMobile = window.innerWidth <= 768;\r\n            const threshold = isMobile ? 0.01 : 0.1;\r\n\r\n            const observer = new IntersectionObserver(this.handleScrollAnimations, {\r\n                root: null,\r\n                rootMargin: '0px',\r\n                threshold: threshold, // Adjust this threshold as needed\r\n            });\r\n\r\n            const section1 = this.$refs.section1;\r\n            const section2 = this.$refs.section2;\r\n\r\n            observer.observe(section1);\r\n            observer.observe(section2);\r\n        },\r\n\r\n    };\r\n</script>\r\n\r\n<style scoped>\r\n\r\n    @import '../assets/styles.css';\r\n  \r\n</style>\r\n\r\n\r\n\r\n\r\n"],"mappings":"AA4GI,OAAOA,GAAE,MAAO,kBAAiB;AACjC,OAAOC,OAAM,MAAO,oBAAoB;AACxC,OAAOC,KAAI,MAAO,sBAAsB;AACxC,OAAOC,IAAG,MAAO,mBAAmB;AAEpC,eAAe;EACXC,IAAI,EAAE,KAAK;EAIXC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHJ,OAAO,EAAEA,OAAO;MAChBC,KAAK,EAAEA,KAAK;MACZC,IAAI,EAAEA,IAAI;MACVG,WAAW,EAAEN,GAAG;MAChBO,aAAa,EAAE,iCAAiC;MAChDC,aAAa,EAAE,kMAAkM;MACjNC,aAAa,EAAE,0NAA0N;MACzOC,aAAa,EAAE,2KAA2K;MAC1LC,aAAa,EAAE,8BAA8B;MAC7CC,aAAa,EAAE,gPAAgP;MAC/PC,aAAa,EAAE;IAEnB,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IAELC,UAAUA,CAAA,EAAG;MACT,IAAI,CAACC,cAAa,GAAI,CAAC,IAAI,CAACA,cAAc;IAC9C,CAAC;IAEDC,sBAAsBA,CAACC,OAAO,EAAE;MAC5BA,OAAO,CAACC,OAAO,CAAEC,KAAK,IAAK;QACvB,IAAIA,KAAK,CAACC,cAAc,EAAE;UACtBD,KAAK,CAACE,MAAM,CAACC,SAAS,CAACC,GAAG,CAAC,SAAS,CAAC;QACzC,OAAO;UACHJ,KAAK,CAACE,MAAM,CAACC,SAAS,CAACE,MAAM,CAAC,SAAS,CAAC;QAC5C;MACJ,CAAC,CAAC;IACN;EACJ,CAAC;EA