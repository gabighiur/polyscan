{"ast":null,"code":"export default {\n  data() {\n    return {\n      isTilted: false\n    };\n  },\n  methods: {\n    openMenu() {\n      this.isTilted = true;\n      document.getElementById(\"container\").addEventListener(\"click\", this.closeMenu);\n      document.querySelector(\".hamburger\").removeEventListener(\"click\", this.openMenu);\n      this.hamburgerFix(true);\n      console.log(\"opening...\");\n    },\n    closeMenu() {\n      this.isTilted = false;\n      document.getElementById(\"container\").removeEventListener(\"click\", this.closeMenu);\n      document.querySelector(\".hamburger\").addEventListener(\"click\", this.openMenu);\n      this.hamburgerFix(false);\n      console.log(\"closing...\");\n    },\n    updateTransformOrigin() {\n      const scrollTop = document.getElementById(\"paper-window\").scrollTop;\n      const equation = (scrollTop + 1800) / document.getElementById(\"paper-front\").offsetHeight * 100;\n      document.getElementById(\"paper-front\").style.transformOrigin = `center ${equation}%`;\n    },\n    hamburgerFix(opening) {\n      const hamburger = document.querySelector(\".hamburger\");\n      if (opening) {\n        hamburger.style.position = \"absolute\";\n        hamburger.style.top = `${document.getElementById(\"paper-window\").scrollTop + 30}px`;\n      } else {\n        setTimeout(() => {\n          hamburger.style.position = \"fixed\";\n          hamburger.style.top = \"30px\";\n        }, 300);\n      }\n    }\n  },\n  mounted() {\n    this.updateTransformOrigin();\n    document.getElementById(\"paper-window\").addEventListener(\"scroll\", this.updateTransformOrigin);\n    document.querySelector(\".hamburger\").addEventListener(\"click\", this.openMenu);\n    document.querySelector(\".close\").addEventListener(\"click\", this.closeMenu);\n  }\n};","map":{"version":3,"names":["data","isTilted","methods","openMenu","document","getElementById","addEventListener","closeMenu","querySelector","removeEventListener","hamburgerFix","console","log","updateTransformOrigin","scrollTop","equation","offsetHeight","style","transformOrigin","opening","hamburger","position","top","setTimeout","mounted"],"sources":["C:\\D\\Vue\\polyscan\\src\\components\\NavBar.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <div id=\"paper-back\">\r\n            <nav>\r\n                <div class=\"close\"></div>\r\n                <a href=\"#\">Home</a>\r\n                <a href=\"#\">About Us</a>\r\n                <a href=\"#\">Our Work</a>\r\n                <a href=\"#\">Contact</a>\r\n            </nav>\r\n        </div>\r\n        <div id=\"paper-window\" :class=\"{ tilt: isTilted }\">\r\n            <div id=\"paper-front\">\r\n                <div class=\"hamburger\" @click=\"openMenu\">\r\n                    <span></span>\r\n                </div>\r\n                <div id=\"container\">\r\n                    <section>\r\n                        <h1>Page Tilt Menu Effect</h1>\r\n                        <p>Click the hamburger icon to see it in action</p>\r\n                    </section>\r\n                    <section></section>\r\n                    <section></section>\r\n                    <section></section>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      isTilted: false,\r\n    };\r\n  },\r\n  methods: {\r\n    openMenu() {\r\n      this.isTilted = true;\r\n      document.getElementById(\"container\").addEventListener(\"click\", this.closeMenu);\r\n      document.querySelector(\".hamburger\").removeEventListener(\"click\", this.openMenu);\r\n      this.hamburgerFix(true);\r\n      console.log(\"opening...\");\r\n    },\r\n    closeMenu() {\r\n      this.isTilted = false;\r\n      document.getElementById(\"container\").removeEventListener(\"click\", this.closeMenu);\r\n      document.querySelector(\".hamburger\").addEventListener(\"click\", this.openMenu);\r\n      this.hamburgerFix(false);\r\n      console.log(\"closing...\");\r\n    },\r\n    updateTransformOrigin() {\r\n      const scrollTop = document.getElementById(\"paper-window\").scrollTop;\r\n      const equation = (scrollTop + 1800) / document.getElementById(\"paper-front\").offsetHeight * 100;\r\n      document.getElementById(\"paper-front\").style.transformOrigin = `center ${equation}%`;\r\n    },\r\n    hamburgerFix(opening) {\r\n      const hamburger = document.querySelector(\".hamburger\");\r\n      if (opening) {\r\n        hamburger.style.position = \"absolute\";\r\n        hamburger.style.top = `${document.getElementById(\"paper-window\").scrollTop + 30}px`;\r\n      } else {\r\n        setTimeout(() => {\r\n          hamburger.style.position = \"fixed\";\r\n          hamburger.style.top = \"30px\";\r\n        }, 300);\r\n      }\r\n    },\r\n  },\r\n  mounted() {\r\n    this.updateTransformOrigin();\r\n    document.getElementById(\"paper-window\").addEventListener(\"scroll\", this.updateTransformOrigin);\r\n    document.querySelector(\".hamburger\").addEventListener(\"click\", this.openMenu);\r\n    document.querySelector(\".close\").addEventListener(\"click\", this.closeMenu);\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n    body {\r\n        font-family: 'Architects Daughter', sans-serif;\r\n    }\r\n\r\n    #paper-back {\r\n        width: 100%;\r\n        height: 100vh;\r\n        background-color: #243040;\r\n        position: fixed;\r\n        top: 0;\r\n        left: 0;\r\n        font-size: 33px;\r\n    }\r\n\r\n    nav {\r\n        padding: 120px 34px;\r\n    }\r\n\r\n    a {\r\n        display: block;\r\n        margin-bottom: 25px;\r\n        text-decoration: none;\r\n        color: rgba(255, 255, 255, 0.7);\r\n    }\r\n\r\n    #paper-window {\r\n        height: 100vh;\r\n        width: 100vw;\r\n        position: relative;\r\n        overflow-x: hidden;\r\n        overflow-y: scroll;\r\n        z-index: 2;\r\n    }\r\n\r\n        #paper-window.tilt {\r\n            overflow: hidden;\r\n            pointer-events: none;\r\n        }\r\n\r\n    #paper-front {\r\n        pointer-events: auto;\r\n        position: relative;\r\n        z-index: 3;\r\n        background-color: white;\r\n        box-shadow: 0 0 20px rgba(0, 0, 0, 0.7);\r\n        transform-origin: center 70%;\r\n        transition: all 0.3s ease;\r\n    }\r\n\r\n    #container {\r\n        section\r\n\r\n    {\r\n        height: 600px;\r\n        text-align: center;\r\n    }\r\n\r\n    section:first-of-type {\r\n        padding-top: 10vh;\r\n    }\r\n\r\n        section:first-of-type h1 {\r\n            font-size: 45px;\r\n        }\r\n\r\n        section:first-of-type p {\r\n            font-size: 25px;\r\n        }\r\n\r\n    @media (max-width: 600px) {\r\n        section:first-of-type {\r\n            padding-top: 15vh;\r\n        }\r\n\r\n            section:first-of-type h1 {\r\n                font-size: 30px;\r\n            }\r\n\r\n            section:first-of-type p {\r\n                font-size: 18px;\r\n            }\r\n    }\r\n\r\n    section:nth-of-type(2n) {\r\n        background-color: lighten(#243040, 75%);\r\n    }\r\n\r\n    }\r\n\r\n    .hamburger {\r\n        position: fixed;\r\n        z-index: 4;\r\n        top: 30px;\r\n        left: 30px;\r\n        width: 45px;\r\n        height: 34px;\r\n        cursor: pointer;\r\n        user-select: none;\r\n    }\r\n\r\n        .hamburger span,\r\n        .hamburger span:before,\r\n        .hamburger span:after {\r\n            display: block;\r\n            width: 45px;\r\n            height: 6px;\r\n            background-color: #243040;\r\n            border-radius: 2px;\r\n        }\r\n\r\n            .hamburger span:before,\r\n            .hamburger span:after {\r\n                content: \"\";\r\n                position: absolute;\r\n            }\r\n\r\n            .hamburger span:before {\r\n                bottom: -14px;\r\n            }\r\n\r\n            .hamburger span:after {\r\n                bottom: -28px;\r\n            }\r\n\r\n    .close {\r\n        position: fixed;\r\n        top: 30px;\r\n        left: 30px;\r\n        width: 45px;\r\n        height: 34px;\r\n        cursor: pointer;\r\n    }\r\n\r\n        .close:before,\r\n        .close:after {\r\n            content: \"\";\r\n            position: absolute;\r\n            display: block;\r\n            width: 45px;\r\n            height: 6px;\r\n            top: 50%;\r\n            background-color: white;\r\n            border-radius: 2px;\r\n        }\r\n\r\n        .close:before {\r\n            transform: translateY(-50%) rotate(45deg);\r\n        }\r\n\r\n        .close:after {\r\n            transform: translateY(-50%) rotate(-45deg);\r\n        }\r\n</style>\r\n"],"mappings":"AA+BA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,QAAQA,CAAA,EAAG;MACT,IAAI,CAACF,QAAO,GAAI,IAAI;MACpBG,QAAQ,CAACC,cAAc,CAAC,WAAW,CAAC,CAACC,gBAAgB,CAAC,OAAO,EAAE,IAAI,CAACC,SAAS,CAAC;MAC9EH,QAAQ,CAACI,aAAa,CAAC,YAAY,CAAC,CAACC,mBAAmB,CAAC,OAAO,EAAE,IAAI,CAACN,QAAQ,CAAC;MAChF,IAAI,CAACO,YAAY,CAAC,IAAI,CAAC;MACvBC,OAAO,CAACC,GAAG,CAAC,YAAY,CAAC;IAC3B,CAAC;IACDL,SAASA,CAAA,EAAG;MACV,IAAI,CAACN,QAAO,GAAI,KAAK;MACrBG,QAAQ,CAACC,cAAc,CAAC,WAAW,CAAC,CAACI,mBAAmB,CAAC,OAAO,EAAE,IAAI,CAACF,SAAS,CAAC;MACjFH,QAAQ,CAACI,aAAa,CAAC,YAAY,CAAC,CAACF,gBAAgB,CAAC,OAAO,EAAE,IAAI,CAACH,QAAQ,CAAC;MAC7E,IAAI,CAACO,YAAY,CAAC,KAAK,CAAC;MACxBC,OAAO,CAACC,GAAG,CAAC,YAAY,CAAC;IAC3B,CAAC;IACDC,qBAAqBA,CAAA,EAAG;MACtB,MAAMC,SAAQ,GAAIV,QAAQ,CAACC,cAAc,CAAC,cAAc,CAAC,CAACS,SAAS;MACnE,MAAMC,QAAO,GAAI,CAACD,SAAQ,GAAI,IAAI,IAAIV,QAAQ,CAACC,cAAc,CAAC,aAAa,CAAC,CAACW,YAAW,GAAI,GAAG;MAC/FZ,QAAQ,CAACC,cAAc,CAAC,aAAa,CAAC,CAACY,KAAK,CAACC,eAAc,GAAK,UAASH,QAAS,GAAE;IACtF,CAAC;IACDL,YAAYA,CAACS,OAAO,EAAE;MACpB,MAAMC,SAAQ,GAAIhB,QAAQ,CAACI,aAAa,CAAC,YAAY,CAAC;MACtD,IAAIW,OAAO,EAAE;QACXC,SAAS,CAACH,KAAK,CAACI,QAAO,GAAI,UAAU;QACrCD,SAAS,CAACH,KAAK,CAACK,GAAE,GAAK,GAAElB,Q