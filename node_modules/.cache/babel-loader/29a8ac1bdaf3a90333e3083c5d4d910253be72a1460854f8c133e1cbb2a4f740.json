ss=\"content-container\">\r\n            <div class=\"fade-in-container\" data-aos=\"fade-up\" data-aos-offset=\"200\">\r\n                <h2 class=\"what-can-i-do\" v-if=\"showWhatCanIDo\">What can I do?</h2>\r\n                <!-- Add your other utilities here -->\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import AOS from 'aos';\r\n    import 'aos/dist/aos.css';\r\n\r\n    export default {\r\n        name: 'PolygonScanner',\r\n        data() {\r\n            return {\r\n                showWhatCanIDo: false,\r\n            };\r\n        },\r\n        mounted() {\r\n            AOS.init({\r\n                once: true,\r\n                offset: 200,\r\n            });\r\n            window.addEventListener('scroll', this.handleScroll);\r\n        },\r\n        beforeUnmounted() {\r\n            window.removeEventListener('scroll', this.handleScroll);\r\n        },\r\n        methods: {\r\n            toggleMenu() {\r\n                // Implement the logic to toggle the mobile menu here\r\n            },\r\n            handleScroll() {\r\n                const videoHeight = this.$el.querySelector('.header-video').offsetHeight;\r\n                const triggerOffset = this.$el.offsetTop + videoHeight;\r\n\r\n                if (window.pageYOffset >= triggerOffset) {\r\n                    this.showWhatCanIDo = true;\r\n                    window.removeEventListener('scroll', this.handleScroll);\r\n                }\r\n            },\r\n        },\r\n    };\r\n</script>\r\n\r\n<style scoped>\r\n    .app {\r\n        background: #27082B;\r\n        color: #67c4a5;\r\n        min-height: 100vh;\r\n        display: flex;\r\n        flex-direction: column;\r\n        align-items: center;\r\n    }\r\n\r\n    .navbar {\r\n        position: fixed;\r\n        top: 0;\r\n        left: 0;\r\n        width: 100%;\r\n        background-color: transparent;\r\n        color: #fff;\r\n        padding: 0.2rem;\r\n        display: flex;\r\n        justify-content: flex-end;\r\n        align-items: center;\r\n        z-index: 9999;\r\n    }\r\n\r\n    .navbar-content {\r\n        display: flex;\r\n        align-items: center;\r\n        justify-content: flex-end;\r\n        margin-right: 15px;\r\n    }\r\n\r\n    .logo {\r\n        width: 120px;\r\n        height: 120px;\r\n    }\r\n\r\n    .invite-button {\r\n        background-color: #67c4a5;\r\n        color: #fff;\r\n        border: none;\r\n        font-size: 1rem;\r\n        padding: 0.5rem 1rem;\r\n        margin-left: 1rem;\r\n        cursor: pointer;\r\n        border-radius: 10px;\r\n    }\r\n\r\n    .hamburger {\r\n        display: none;\r\n        background-color: transparent;\r\n        border: none;\r\n        cursor: pointer;\r\n    }\r\n\r\n    .hamburger-line {\r\n        display: block;\r\n        width: 30px;\r\n        height: 2px;\r\n        background-color: #fff;\r\n        margin: 5px;\r\n    }\r\n\r\n    .header {\r\n        position: relative;\r\n        display: flex;\r\n        justify-content: center;\r\n        align-items: center;\r\n    }\r\n    .header-video {\r\n        max-width: 60%;\r\n        height: auto;\r\n        object-fit: cover;\r\n    }\r\n\r\n    .content-container {\r\n        margin-top: 50vh;\r\n        display: flex;\r\n        justify-content: center;\r\n        align-items: flex-start;\r\n    }\r\n\r\n    .what-can-i-do {\r\n        opacity: 0;\r\n    }\r\n\r\n    .fade-in-container[data-aos='fade-up'] {\r\n        opacity: 0;\r\n    }\r\n\r\n        .fade-in-container[data-aos='fade-up'].aos-animate {\r\n            opacity: 1;\r\n        }\r\n</style>\r\n"],"mappings":"AA0BI,OAAOA,GAAE,MAAO,KAAK;AACrB,OAAO,kBAAkB;AAEzB,eAAe;EACXC,IAAI,EAAE,gBAAgB;EACtBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,cAAc,EAAE;IACpB,CAAC;EACL,CAAC;EACDC,OAAOA,CAAA,EAAG;IACNJ,GAAG,CAACK,IAAI,CAAC;MACLC,IAAI,EAAE,IAAI;MACVC,MAAM,EAAE;IACZ,CAAC,CAAC;IACFC,MAAM,CAACC,gBAAgB,CAAC,QAAQ,EAAE,IAAI,CAACC,YAAY,CAAC;EACxD,CAAC;EACDC